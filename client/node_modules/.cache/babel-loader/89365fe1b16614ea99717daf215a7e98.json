   if (id) {\n      this.current_plan = id;\n    } else {\n      this.current_plan = 0;\n    }\n  },\n  methods: {\n    clickSwitch: function clickSwitch() {\n      this.switch_annual = !this.switch_annual;\n    },\n    chooseThisPlan: function chooseThisPlan(toPlan, id) {\n      if (this.switch_annual == true) {\n        this.plan_mode = \"annual\";\n      } else {\n        this.plan_mode = \"monthly\";\n      }\n\n      this.$router.push({\n        path: \"/payment/upgrade-to-plan\",\n        query: {\n          plan: toPlan,\n          id: id,\n          planmode: this.plan_mode\n        }\n      });\n    }\n  }\n};",{"version":3,"sources":["UpgradeYourPlan.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2DA,OAAA,QAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,iBADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,QAAA,EAAA;AADA,GAFA;AAKA,EAAA,IALA,kBAKA;AACA,WAAA;AACA,MAAA,YAAA,EAAA,CADA;AAEA,MAAA,aAAA,EAAA,IAFA;AAGA,MAAA,SAAA,EAAA,EAHA;AAIA,MAAA,KAAA,EAAA,CACA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,KAAA,EAAA,CAFA;AAGA,QAAA,OAAA,EAAA,CAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OADA,EAOA;AACA,QAAA,KAAA,EAAA,WADA;AAEA,QAAA,KAAA,EAAA,EAFA;AAGA,QAAA,OAAA,EAAA,CAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAPA,EAaA;AACA,QAAA,KAAA,EAAA,UADA;AAEA,QAAA,KAAA,EAAA,EAFA;AAGA,QAAA,OAAA,EAAA,EAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAbA,EAmBA;AACA,QAAA,KAAA,EAAA,KADA;AAEA,QAAA,KAAA,EAAA,EAFA;AAGA,QAAA,OAAA,EAAA,EAHA;AAIA,QAAA,KAAA,EAAA;AAJA,OAnBA;AAJA,KAAA;AA+BA,GArCA;AAsCA,EAAA,OAtCA,qBAsCA;AACA,QAAA,EAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,EAAA;;AACA,QAAA,EAAA,EAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,KAFA,MAEA;AACA,WAAA,YAAA,GAAA,CAAA;AACA;AACA,GA7CA;AA8CA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,aAAA,GAAA,CAAA,KAAA,aAAA;AACA,KAHA;AAIA,IAAA,cAJA,0BAIA,MAJA,EAIA,EAJA,EAIA;AACA,UAAA,KAAA,aAAA,IAAA,IAAA,EAAA;AACA,aAAA,SAAA,GAAA,QAAA;AACA,OAFA,MAEA;AACA,aAAA,SAAA,GAAA,SAAA;AACA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,0BADA;AAEA,QAAA,KAAA,EAAA;AAAA,UAAA,IAAA,EAAA,MAAA;AAAA,UAAA,EAAA,EAAA,EAAA;AAAA,UAAA,QAAA,EAAA,KAAA;AAAA;AAFA,OAAA;AAIA;AAdA;AA9CA,CAAA","sourcesContent":["<template>\n  <div class=\"app flex-row\">\n    <div class=\"w-100 position-relative\">\n      <div class=\"plan-header\">\n        <h1>Upgrade {{plans[current_plan].title}} Plan</h1>\n        <div class=\"switch-button\" v-on:click=\"clickSwitch()\">\n          <div class=\"switch-item\" v-bind:class=\"{'selected': switch_annual}\">Annual</div>\n          <div class=\"switch-item\" v-bind:class=\"{'selected': !switch_annual}\">Monthly</div>\n        </div>\n      </div>\n      <hr class=\"mb-4\" />\n      <div class=\"row plans px-2\">\n        <div\n          class=\"col-12 col-sm-6 col-md-6 col-lg-3 px-1\"\n          v-for=\"(plan, index) in plans\"\n          :key=\"index\"\n        >\n          <div class=\"plan-card content-card\">\n            <div class=\"plan-card-head\">\n              <div class=\"plan-title\">{{plan.title}}</div>\n              <!-- <UserIcon :icon=\"plan.title + '.png'\" :height=\"26\" :width=\"26\" /> -->\n              <img v-bind:src=\"'img/icons/'+plan.title+'.png'\" width=\"32\" height=\"32\" />\n            </div>\n            <div class=\"plan-card-head mt-4\">\n              <div class=\"comments\">\n                <div v-if=\"index==0\">Per month</div>\n                <div v-if=\"index==1\">Per user</div>\n                <div v-if=\"index>1\">per user, per month</div>\n                <div v-if=\"(switch_annual && (plan.price>0))\">${{plan.price*12}} annually</div>\n              </div>\n              <span class=\"price\">\n                ${{switch_annual?plan.price:plan.monthly}}\n                <sup>.00</sup>\n              </span>\n            </div>\n            <hr />\n            <div class=\"text-center\">\n              <div class=\"limit-for-sending\">Limit for Sending for Signature</div>\n              <div class=\"limit-times\">{{plan.limit}}</div>\n            </div>\n            <hr />\n            <b-button\n              block\n              :variant=\"current_plan==index?'secondary':'other'\"\n              :disabled=\"current_plan==index\"\n              v-on:click=\"chooseThisPlan(plan, index)\"\n            >{{current_plan == index?\"Current plan\":\"Choose this plan\"}}</b-button>\n          </div>\n        </div>\n      </div>\n      <div class=\"plan-cancel\">\n        <i class=\"fa fa-close\"></i>\n        <b-button variant=\"link\">Cancel plan</b-button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport UserIcon from \"../../components/UserIcon\";\n\nexport default {\n  name: \"UpgradeYourPlan\",\n  components: {\n    UserIcon\n  },\n  data() {\n    return {\n      current_plan: 0,\n      switch_annual: true,\n      plan_mode: \"\",\n      plans: [\n        {\n          title: \"Free\",\n          price: 0,\n          monthly: 0,\n          limit: \"5 Times total\"\n        },\n        {\n          title: \"Personnel\",\n          price: 10,\n          monthly: 5,\n          limit: \"5 Times total\"\n        },\n        {\n          title: \"Standard\",\n          price: 25,\n          monthly: 10,\n          limit: \"30 Times total\"\n        },\n        {\n          title: \"Pro\",\n          price: 40,\n          monthly: 20,\n          limit: \"No Limit ∞\"\n        }\n      ]\n    };\n  },\n  created() {\n    const id = this.$route.query.id;\n    if (id) {\n      this.current_plan = id;\n    } else {\n      this.current_plan = 0;\n    }\n  },\n  methods: {\n    clickSwitch() {\n      this.switch_annual = !this.switch_annual;\n    },\n    chooseThisPlan(toPlan, id) {\n      if (this.switch_annual==true) {\n         this.plan_mode = \"annual\";  \n      } else {\n        this.plan_mode = \"monthly\"\n      }\n      this.$router.push({\n        path: \"/payment/upgrade-to-plan\",\n        query: { plan: toPlan, id: id, planmode: this.plan_mode }\n      });\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n@import \"./UpgradeYourPlan.scss\";\n</style>\n\n"],"sourceRoot":"src/views/payment"}]}